{
  "name": "ace-prep",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev -w @ace-prep/server\" \"npm run dev -w @ace-prep/client\"",
    "dev:server": "npm run dev -w @ace-prep/server",
    "dev:client": "npm run dev -w @ace-prep/client",
    "build": "npm run build -w @ace-prep/shared && npm run build -w @ace-prep/server && npm run build -w @ace-prep/client",
    "test": "npm run test -w @ace-prep/server",
    "test:watch": "npm run test:watch -w @ace-prep/server",
    "test:coverage": "npm run test:coverage -w @ace-prep/server",
    "db:setup": "npm run db:setup -w @ace-prep/server",
    "db:generate": "npm run db:generate -w @ace-prep/server",
    "db:migrate": "npm run db:migrate -w @ace-prep/server",
    "db:seed": "npm run db:seed -w @ace-prep/server"
  },
  "engines": {
    "node": "22.2.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.6.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
