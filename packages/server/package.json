{
  "name": "@ace-prep/server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "db:setup": "tsx src/db/setup.ts && npm run db:seed-case-studies",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "db:seed-case-studies": "tsx src/db/seedCaseStudies.ts",
    "db:add-pca": "tsx src/db/migrations/0002_add_pca_certification.ts"
  },
  "dependencies": {
    "@ace-prep/shared": "*",
    "@anthropic-ai/sdk": "^0.71.2",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.2.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^9.0.0",
    "@sentry/node": "^10.36.0",
    "better-sqlite3": "^12.6.2",
    "drizzle-orm": "^0.45.1",
    "fastify": "^5.7.1",
    "jsonwebtoken": "^9.0.3",
    "node-cron": "^4.2.1",
    "openai": "^6.16.0",
    "pdfkit": "^0.17.2",
    "web-push": "^3.6.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.10",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.17.4",
    "@types/web-push": "^3.6.4",
    "@vitest/coverage-v8": "^4.0.18",
    "drizzle-kit": "^0.31.8",
    "pino-pretty": "^13.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^4.0.16"
  }
}
